def solve_knapsack():
 val = list(map(int, input("Enter values").split()))
 wt = list(map(int, input("Enter weights").split()))
 W = int(input("Enter capacity of knapsack: "))
 n = len(val) - 1
 def knapsack(W, n):
 if n < 0 or W <= 0:
 return 0
 if wt[n] > W:
 return knapsack(W, n - 1)
 else:
 return max(val[n] + knapsack(W - wt[n], n - 1), knapsack(W, n
- 1))
 print("Maximum value in knapsack is:", knapsack(W, n))
if __name__ == "__main__":
 solve_knapsack()
